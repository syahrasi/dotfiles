  ai = add -i
  ap = add -p
  co = checkout
  ci = commit
  cm = commit -m
  cam = commit -am
  st = status
  sb = status -sb
  cb = checkout -b
  cf = checkout --
  ba = branch -a
  br = branch -v
  d = diff
  dc = diff --cached
  dw = diff --word-diff=color
  dl = diff HEAD^
  hist = log --pretty=tformat:'%C(bold red)%h%Creset %ad | %Cgreen%an%Creset %Cblue%s%Creset%C(red)%d%Creset' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
